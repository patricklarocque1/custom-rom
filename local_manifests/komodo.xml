<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- AOSP remote and default revision for Android 16. Adjust r-tag as needed. -->
  <remote name="aosp" fetch="https://android.googlesource.com/" />
  <default remote="aosp" revision="android-16.0.0_r1" />

  <!-- Pixel 9 Pro XL (komodo) uses the Tensor G4 kernel family. The kernel
       source below points at the "caimito" GKI branch used for Android 16.
       You can change the revision to a newer android16 kernel tag later. -->
  <project name="kernel/gs/caimito"
           path="kernel/google/gs/caimito"
           revision="android-gs-caimito-6.1-android16" />

  <!-- Device and vendor repositories for Pixel 9 Pro XL (komodo) -->
  <remote name="github" fetch="https://github.com/" />
  <remote name="lineage" fetch="https://github.com/LineageOS/" />
  <remote name="muppets" fetch="https://github.com/TheMuppets/" />
  
  <!-- Device tree for Pixel 9 Pro XL -->
  <project name="LineageOS/android_device_google_komodo"
           path="device/google/komodo"
           remote="lineage"
           revision="lineage-22.0" />
  
  <!-- Common device tree for Pixel 9 family -->
  <project name="LineageOS/android_device_google_caimito"
           path="device/google/caimito"
           remote="lineage"
           revision="lineage-22.0" />
  
  <!-- Vendor blobs for Google devices -->
  <project name="TheMuppets/proprietary_vendor_google"
           path="vendor/google"
           remote="muppets"
           revision="lineage-22.0" />
  
  <!-- Hardware abstraction layer -->
  <project name="LineageOS/android_hardware_google_pixel"
           path="hardware/google/pixel"
           remote="lineage"
           revision="lineage-22.0" />
  
  <!-- Additional Tensor G4 specific repositories -->
  <project name="LineageOS/android_hardware_google_graphics_zuma"
           path="hardware/google/graphics/zuma"
           remote="lineage"
           revision="lineage-22.0" />
</manifest>

